{
 "name": "View Password",
 "description": "Extension for creating 'eyes' to see password",
 "version": "1.0",
 "manifest_version": 3,
 "content_scripts":[
  {
   "matches":["https://*/*", "http://*/*"],
   "css":["style.css"],
   "js":["script.js"]
  }
 ],
 "web_accessible_resources":[{
      "resources": ["eye.svg","eye_slash.svg"],
      "matches": ["https://*/*", "http://*/*"]
    }]
}
